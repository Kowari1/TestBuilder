<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Test_Builder.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Test_Builder"
    xmlns:pages="clr-namespace:Test_Builder.Pages"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        Title=""
        ContentTemplate="{DataTemplate pages:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="CreateTestPage"
        ContentTemplate="{DataTemplate pages:CreateTestPage}"
        Route="CreateTestPage" />
</Shell>
